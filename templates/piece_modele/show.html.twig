    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ p06_piece_modele.id }}</td>
            </tr>
            <tr>
                <th>PieceVersion</th>
                <td>{{ p06_piece_modele.PieceVersion }}</td>
            </tr>
            <tr>
                <th>PieceValeur</th>
